{
  "name": "handy-variation-swatches",
  "version": "1.0.0",
  "description": "A WordPress plugin for adding variation swatches in WooCommerce.",
  "main": "app.js",
  "scripts": {
    "lint-admin-scss": "wp-scripts lint-style assets/admin/src/scss/**/*.scss",
    "lint-admin-scss-fix": "wp-scripts lint-style assets/admin/src/scss/**/*.scss --fix",
    "lint-client-scss": "wp-scripts lint-style assets/client/src/scss/**/*.scss",
    "lint-client-scss-fix": "wp-scripts lint-style assets/client/src/scss/**/*.scss --fix",
    "lint-admin-js": "wp-scripts lint-js assets/admin/src/js/**/*.js",
    "lint-admin-js-fix": "wp-scripts lint-js assets/admin/src/js/**/*.js --fix",
    "lint-client-js": "wp-scripts lint-js assets/client/src/js/**/*.js",
    "lint-client-js-fix": "wp-scripts lint-js assets/client/src/js/**/*.js --fix"
  },
